CREATE DEFINER=`` PROCEDURE `getUserFullPreference`(
	in in_userid INT(10)
)
BEGIN
	select 
		up.userID, 
        cc.clip_id_1,
        cc.clip_id_2,
        up.rating
    from 
		userpreference as up,
        clipcliplink as cc
    where
		up.userID = in_userid
        &&
        up.clipcliplinkID = cc.id
    ;

END